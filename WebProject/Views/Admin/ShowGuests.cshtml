@using WebProject.Models
@{
    Layout = null;
}

<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
<body style="background-image:url(https://r-cf.bstatic.com/images/hotel/max1024x768/119/119673250.jpg); height: 100%; background-position:center; background-repeat: no-repeat; background-size: cover">
    <div class="w3-bar w3-border w3-transparent w3-border-black">
        <a href="/Admin/Index" class="w3-bar-item w3-button w3-text-white">Home page</a>
    </div>
    <table border="1" style="background-color:transparent" class="w3-text-white w3-large">
        <tr>
            <th>
                Name
            </th>
            <th>
                Surname
            </th>
            <th>
                Username
            </th>
            <th>
                Gender
            </th>
         </tr>
        @foreach (KeyValuePair<string, Guest> kv in ViewBag.guests)
        {
            <tr>
                <td>
                    @kv.Value.Name
                </td>
                <td>
                    @kv.Value.Surname
                </td>
                <td>
                    @kv.Value.Username
                </td>
                <td>
                    @kv.Value.Gender
                </td>
             </tr>
         }
        
</table>
    
</body>
